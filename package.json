{
  "name": "@commercio.network/vue-commercio-ui",
  "version": "0.2.0",
  "description": "A Vue.js SFC components library",
  "main": "dist/vue-commercio-ui.ssr.js",
  "module": "dist/vue-commercio-ui.esm.js",
  "unpkg": "dist/vue-commercio-ui.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/lib-dev.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "cross-env": "^5.2.1",
    "minimist": "^1.2.0",
    "node-sass": "^4.12.0",
    "rollup": "^1.20.3",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "sass-loader": "^7.3.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Commercionetwork/vue-commercio-ui.git"
  },
  "author": "Commerc.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Commercionetwork/vue-commercio-ui/issues"
  },
  "homepage": "https://github.com/Commercionetwork/vue-commercio-ui#readme"
}
